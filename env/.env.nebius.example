# Nebius AI Configuration
# Add these environment variables to your .env.local file

# Nebius AI Studio API Key (Required)
# Get your API key from: https://studio.nebius.com/account/api-keys
NEXT_PUBLIC_NEBIUS_API_KEY=your_nebius_api_key_here

# Nebius Configuration (Optional - defaults provided)
NEBIUS_BASE_URL=https://api.studio.nebius.com/v1
NEBIUS_CODE_MODEL=deepseek-v3
NEBIUS_ARCHITECTURE_MODEL=llama-3.3-70b-instruct
NEBIUS_OPTIMIZATION_MODEL=qwen-2.5-coder-32b

# Generation Settings (Optional)
DEFAULT_GENERATION_TIMEOUT=300000  # 5 minutes in milliseconds
MAX_CONCURRENT_GENERATIONS=3
ENABLE_GENERATION_CACHING=true

# Deployment Settings (Optional)
GITHUB_TOKEN=your_github_token
VERCEL_TOKEN=your_vercel_token
DEFAULT_DEPLOYMENT_PROVIDER=vercel

# App Card Settings (Optional)
ENABLE_BATTLE_STATS=true
ENABLE_RARITY_SYSTEM=true
MIN_QUALITY_SCORE=70
TRADING_VALUE_MULTIPLIER=10

# Development Settings (Optional)
ENABLE_GENERATION_SIMULATION=false  # Set to true to use simulation instead of real API
VERBOSE_LOGGING=true
DEBUG_NEBIUS_REQUESTS=false
